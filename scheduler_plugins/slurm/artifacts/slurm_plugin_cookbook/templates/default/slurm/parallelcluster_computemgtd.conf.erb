proxy = <%= node['pcluster']['proxy'] %>
clustermgtd_heartbeat_file_path = <%= node['pcluster']['shared_dir'] %>/.slurm_plugin/clustermgtd_heartbeat
dynamodb_table = <%= node['pcluster']['cfn_stack_outputs']['Outputs']['DynamoDBTable'] %>
slurm_nodename_file = <%= node['pcluster']['local_dir'] %>/slurm_nodename
