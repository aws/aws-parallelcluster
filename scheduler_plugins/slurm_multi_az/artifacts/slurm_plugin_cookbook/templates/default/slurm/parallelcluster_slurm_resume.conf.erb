[slurm_resume]
cluster_name = <%= node['pcluster']['cluster_name'] %>
region = <%= node['pcluster']['region'] %>
proxy = <%= node['pcluster']['proxy'] %>
dynamodb_table = <%= node['pcluster']['cfn_stack_outputs']['Outputs']['DynamoDBTable'] %>
hosted_zone = <%= node['pcluster']['cfn_stack_outputs']['Outputs']['Route53HostedZone'] %>
dns_domain = <%= node['dns']['domain'] %>
head_node_private_ip = <%= node['ec2']['local_ipv4'] %>
head_node_hostname = <%= node['ec2']['local_hostname'] %>
clustermgtd_heartbeat_file_path = <%= node['slurm']['install_dir'] %>/etc/pcluster/.slurm_plugin/clustermgtd_heartbeat
instance_type_mapping = <%= node['slurm']['install_dir'] %>/etc/pcluster/instance_name_type_mappings.json
run_instances_overrides = <%= node['slurm']['install_dir'] %>/etc/pcluster/run_instances_overrides.json
